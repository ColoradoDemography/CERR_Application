{
    "extends": "eslint:recommended",
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "globals": {
      "Papa": false,
      "saveAs": false
    },
    "plugins": ["babel","flow-vars"],
    "rules": {
      "no-console": 0,
      "flow-vars/define-flow-type": 1,
      "flow-vars/use-flow-type": 1
    }
}
